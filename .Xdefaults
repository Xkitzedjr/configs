! solarized theme
! foreground/background
! black
URxvt.color0  : #000000
URxvt.color8  : #555555
! red
URxvt.color1  : #AA0000
URxvt.color9  : #FF5555
! green
URxvt.color2  : #00AA00
URxvt.color10 : #55FF55
! yellow
URxvt.color3  : #AB4400
URxvt.color11 : #FFFF44
! blue
URxvt.color4  : #0000AA
URxvt.color12 : #5555FF
! magenta
URxvt.color5  : #C400C4
URxvt.color13 : #FF88FF
! cyan
URxvt.color6  : #00AAAA
URxvt.color14 : #55FFFF
! white
URxvt.color7  : #AAAAAA
! transparency
URxvt*background: [80]#000010
URxvt*depth: 32
URxvt*foreground: #FFFFFF
! font
URxvt.font: xft:terminu:size=13
URxvt.cursorBlink: true
URxvt.internalBorder: 12
URxvt.scrollBar: false
URxvt.letterSpace: -1

!st simple terminal
!font
st.font: xos4 Terminus:size=12
st.cursorBlink: true
st.alpha: 0.9
st.background: #000000
!black
st.color0  : #151515
st.color8  : #a6a8a1
! red
st.color1  : #cc0000
st.color9  : #ff0000
! green
st.color2  : #00e500
st.color10 : #00ff00
! yellow
st.color3  : #ffff00
st.color11 : #ffff00
! blue
st.color4  : #4d4dff
st.color12 : #0080ff
! magenta
st.color5  : #ffc0cb
st.color13 : #ff748c
! cyan
st.color6  : #12cfc0
st.color14 : #00ffff
! white
st.color7  : #ffffff
st.color15 : #ffffff

