! solarized theme
! foreground/background
! black
URxvt.color0  : #000000
URxvt.color8  : #555555
! red
URxvt.color1  : #AA0000
URxvt.color9  : #FF5555
! green
URxvt.color2  : #00AA00
URxvt.color10 : #55FF55
! yellow
URxvt.color3  : #AB4400
URxvt.color11 : #FFFF44
! blue
URxvt.color4  : #0000AA
URxvt.color12 : #5555FF
! magenta
URxvt.color5  : #C400C4
URxvt.color13 : #FF88FF
! cyan
URxvt.color6  : #00AAAA
URxvt.color14 : #55FFFF
! white
URxvt.color7  : #AAAAAA
! transparency
URxvt*background: [80]#000010
URxvt*depth: 32
URxvt*foreground: #FFFFFF
! font
URxvt.font: xft:terminu:size=13
URxvt.cursorBlink: true
URxvt.internalBorder: 12
URxvt.scrollBar: false
URxvt.letterSpace: -1

!st simple terminal
!font
st.font: xos4 Terminus:size=12
st.cursorBlink: true
st.alpha: 0.9
st.foreground: #f8f8f2
st.background: #282a36
!black
st.color0  : #000000
st.color8  : #000000
! red
st.color1  : #ff5555
st.color9  : #ff5555
! green
st.color2  : #50fa7b
st.color10 : #50fa7b
! yellow
st.color3  : #f1fa8c
st.color11 : #f1fa8c
! blue
st.color4  : #6272a4
st.color12 : #6272a4
! magenta
st.color5  : #bd93f9
st.color13 : #bd93f9
! cyan
st.color6  : #8be0fd
st.color14 : #8be0fd
! white
st.color7  : #ffffff
st.color15 : #ffffff

