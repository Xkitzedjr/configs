# A contaiment file for all my aliases

alias ..='cd ..'
alias ls='exa -lh --group-directories-first'
alias el='exa -lah --group-directories-first'
alias tree="ls -T"
alias vi='nvim'
alias dirs='dirs -v'
alias doc='cd ~/Documents'
alias pic='cd ~/Pictures'
alias down='cd ~/Downloads'
alias mktar='tar -cvf'
alias untar='tar -xvf'
alias make='make -j12'
alias cal='cal -3m'
